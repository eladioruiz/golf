<h1>New match</h1>

<% form_for(@match) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :course %><br />
    <%= f.select(:course_id,@courses) %>
  </p>
  <p>
    <%= f.label :date_hour_match %><br />
    <%= f.datetime_select :date_hour_match %>
  </p>
  <p>
    <%= f.label :holes %><br />
    <%= f.text_field :holes %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>

<% end %>

<%= link_to 'Back', matches_path %>