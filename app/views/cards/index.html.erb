<h1>Listing match_cards</h1>

<table>
  <tr>
    <th>Match</th>
    <th>Player</th>
    <th>Hole</th>
    <th>Strokes</th>
  </tr>

<% @match_cards.each do |match_card| %>
  <tr>
    <td><%=h match_card.match %></td>
    <td><%=h match_card.player %></td>
    <td><%=h match_card.hole %></td>
    <td><%=h match_card.strokes %></td>
    <td><%= link_to 'Show', match_card %></td>
    <td><%= link_to 'Edit', edit_match_card_path(match_card) %></td>
    <td><%= link_to 'Destroy', match_card, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New match_card', new_match_card_path %>