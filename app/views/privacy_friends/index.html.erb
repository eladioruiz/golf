<h1>Listing privacy_friends</h1>

<table>
  <tr>
  </tr>

<% @privacy_friends.each do |privacy_friend| %>
  <tr>
    <td><%= privacy_friend.id %></td>
    <td><%= link_to 'Show', privacy_friend %></td>
    <td><%= link_to 'Edit', edit_privacy_friend_path(privacy_friend) %></td>
    <td><%= link_to 'Destroy', privacy_friend, :confirm => '¿Está seguro?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New privacy_friend', new_privacy_friend_path %>